{
  "manifest_version": 2,

  "name": "Booli-exclude",
  "description": "Exclude hits from Booli search",
  "version": "0.1",

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "http://www.booli.se/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
    "content_scripts": [
    {
      "matches": ["http://www.booli.se/*"],
      "js": ["booli-exclude.js"]
    }
  ]
}